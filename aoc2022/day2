var input = open("../test.txt");

var splitInput = split(input);

var p1_map = {
  "A X": 4,
  "A Y": 8,
  "A Z": 3,
  "B X": 1,
  "B Y": 5,
  "B Z": 9,
  "C X": 7,
  "C Y": 2,
  "C Z": 6
};

var p2_map = {
  "A X": 3,
  "A Y": 4,
  "A Z": 8,
  "B X": 1,
  "B Y": 5,
  "B Z": 9,
  "C X": 2,
  "C Y": 6,
  "C Z": 7
};

var p1 = 0;
var p2 = 0;
for(var i = 0; i < len(splitInput); i = i + 1)
{
  p1 = p1 + p1_map[splitInput[i]]; 
  p2 = p2 + p2_map[splitInput[i]]; 
}

print p1;
print p2;
