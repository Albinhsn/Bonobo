var a:int = 5 * 4 * 2 - 1 + 3;
printf("a: %d\n",a);

var b: arr[int] = [];
var c: arr[int] = [1,2,3];

var d: bool = 5 <= 3;
printf("d: %d\n", d);

for(var i: int = 0; i < 5; i = i + 1){
  printf("i: %d\n", i);
}

fun foo(a:int, b:int) -> int{
  return a + b;
}
printf("foo(1,2): %d\n", foo(1,2));

var f: bool = true and false;
printf("f: %d\n", a);

var i:int = 0;
while(i < 5){
  printf("i: %d\n", i);
  i = i + 1;
}
