struct Zoo {
  aardvark;
  baboon;
  cat;
  donkey;
  elephant;
  fox;
};

var zoo = Zoo(1,1,1,1,1,1);
var sum = 0;
var c = clock();
while (sum < 100000000) {
  sum = sum + zoo.aardvark
            + zoo.baboon
            + zoo.cat
            + zoo.donkey
            + zoo.elephant
            + zoo.fox;
}
print clock() - c;
